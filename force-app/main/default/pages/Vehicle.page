<apex:page controller="ApplicationWizard">
 <apex:form >
  <apex:pageblock title="Enter Asset Details">
  <apex:pageBlockSection columns="1">
    <apex:inputField value="{!vehicle.Name}"/>
    <apex:inputField value="{!vehicle.Make__c}"/>
    <apex:inputField value="{!vehicle.Price__c}"/>
    <apex:inputField value="{!vehicle.Type__c}"/>
    <apex:inputField value="{!vehicle.Model__c}"/>
    <apex:inputField value="{!vehicle.Year__c}"/>
  </apex:pageBlockSection>
  <apex:commandButton value="Back" action="{!GoToVehicleList}"/>
   <apex:commandButton value="Save" action="{!AddVehicle}"/>
  </apex:pageblock>
  </apex:form>
</apex:page>